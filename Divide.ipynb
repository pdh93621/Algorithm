   ],
   "source": [
    "count = 0\n",
    "\n",
    "while N != 1:\n",
    "    temp = (N//K) * K\n",
    "    count += N - temp\n",
    "    N = temp\n",
    "    \n",
    "    if K > N:\n",
    "        break\n",
    "    \n",
    "    N //= K\n",
    "    count += 1\n",
    "    \n",
    "count += (N-1)\n",
    "print(count)\n",
